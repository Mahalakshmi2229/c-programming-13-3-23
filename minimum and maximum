#include<stdio.h>
 int main()
{
	int a[100],n,i,max,min;
	printf("enter array size");
	scanf("%d",&n);
	printf("enter array value");
	for(i=0;i<n;i++)
	{
	scanf("%d",&a[i]);
}
	max=min=a[0];
	for(i=0;i<n;i++)
	{
			if(min>a[i])
			min=a[i];
			if(max<a[i])
			max=a[i];
	}
	printf("maximum number of array is %d",&max);
	printf("minimum number of array is %d",&min);
}
